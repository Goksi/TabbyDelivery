<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="399.0" prefWidth="503.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="tech.goksi.projekatop.controllers.LoginController">
    <Label layoutX="125.0" layoutY="69.0" text="Login za nastavak">
        <font>
            <Font size="36.0"/>
        </font>
    </Label>
    <TextField fx:id="usernameField" alignment="CENTER" layoutX="139.0" layoutY="152.0" prefHeight="39.0"
               prefWidth="253.0" promptText="Unesite svoj username..."/>
    <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="139.0" layoutY="227.0" prefHeight="39.0"
                   prefWidth="253.0" promptText="Unesite svoj password..."/>
    <Button layoutX="207.0" layoutY="293.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="39.0"
            prefWidth="118.0" text="Login"/>
</AnchorPane>
